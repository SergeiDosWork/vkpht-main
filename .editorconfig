; top-most EditorConfig file
root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = space
insert_final_newline = false
max_line_length = 120

[*.java]
indent_size = 4
ij_java_imports_layout = *, |, javax.**, java.**, |, com.goodt.**, me.goodt.**, ru.abcconsulting.**, |, $*

[*.sql]
ij_sql_keyword_case = 2
